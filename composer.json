{
    "name": "madpilot78/yii2-otputil",
    "description": "TOTP/HOTP Yii2 utility extension",
    "keywords": [
        "security",
        "totp",
        "hotp",
        "mfa",
        "2fa",
        "yii2",
        "yii",
        "yii 2",
        "extension"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Guido Falsi",
            "email": "mad@madpilot.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "yiisoft/yii2": "~2.0.5",
        "bacon/bacon-qr-code": "~1.0.3",
        "chillerlan/php-authenticator": "^2.0.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.0|^7.0",
        "fzaninotto/faker": "^1.7.0",
        "khanamiryan/qrcode-detector-decoder": "^1.0.2"
    },
    "autoload": {
        "psr-4": {
            "madpilot78\\otputil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "madpilot78\\otputil\\tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
