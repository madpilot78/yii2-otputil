{
    "name": "mad/yii2-otputil",
    "description": "TOTP/HOTP Yii2 utility extension",
    "keywords": [
        "security",
        "totp",
        "hotp",
        "mfa",
        "2fa",
        "yii2",
        "yii",
        "yii 2",
        "extension"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.0.0",
        "yiisoft/yii2": "~2.0.5",
        "bacon/bacon-qr-code": "~1.0.3",
        "chillerlan/php-authenticator": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.0",
        "phpunit/dbunit": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "yii2-otputil": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
